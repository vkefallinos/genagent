{
  "name": "genagent",
  "version": "0.1.0",
  "description": "A library for creating AI agents with terminal UI using AI SDK and Ink",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "example": "node --loader ts-node/esm examples/basic.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "keywords": [
    "ai",
    "agent",
    "terminal",
    "ink",
    "ai-sdk"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai": "^1.0.0",
    "ai": "^4.0.0",
    "dotenv": "^16.4.5",
    "ink": "^5.0.1",
    "marked": "^17.0.1",
    "marked-terminal": "^7.3.0",
    "react": "^18.3.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.3",
    "typescript": "^5.5.4",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2"
  }
}
